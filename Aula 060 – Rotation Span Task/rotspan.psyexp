        <Param val="" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="thisExp.addData(&quot;rotation_time_mean&quot;, rotation_time_mean)&amp;#10;thisExp.addData(&quot;rotation_time_sd&quot;, rotation_time_sd)&amp;#10;thisExp.addData(&quot;rotation_criterion&quot;, rotation_criterion)" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
